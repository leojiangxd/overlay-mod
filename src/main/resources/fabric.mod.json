{
	"schemaVersion": 1,
	"id": "overlay-mod",
	"version": "${version}",
	"name": "Overlay Mod",
	"description": "Simple overlay mod",
	"authors": [
		"Triangled"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/overlay-mod",
		"sources": "https://github.com/leojiangxd/overlay-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/overlay-mod/icon.png",
	"environment": "*",
