{
	"schemaVersion": 1,
	"id": "overlay-mod",
	"version": "${version}",
	"name": "Overlay Mod",
	"description": "Simple overlay mod",
	"authors": [
		"Triangled"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/overlay-mod/icon.png",
