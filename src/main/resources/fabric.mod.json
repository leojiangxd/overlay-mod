	"entrypoints": {
		"main": [
			"com.triangled.overlaymod.OverlayMod"
		],
		"fabric-datagen": [
			"com.triangled.overlaymod.OverlayModDataGenerator"
		],
		"client" : [
			"com.triangled.overlaymod.OverlayModClient"
		]
	},
	"mixins": [
		"overlay-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.1",
		"java": ">=21",
